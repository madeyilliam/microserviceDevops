<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingGuestRules>
        <fullName>Show_Courses_for_Guest_Users_PRO</fullName>
        <accessLevel>Read</accessLevel>
        <description>Used with Salesforce CMS collection. Shows guest users all published courses on the PROFESSIONAL community.</description>
        <label>Show Courses for Guest Users: PRO</label>
        <sharedTo>
            <guestUser>SDO_Professional</guestUser>
        </sharedTo>
        <criteriaItems>
            <field>Course_Status__c</field>
            <operation>equals</operation>
            <value>Available for Registration</value>
        </criteriaItems>
        <includeHVUOwnedRecords>false</includeHVUOwnedRecords>
    </sharingGuestRules>
</SharingRules>
